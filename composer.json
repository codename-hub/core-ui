{
    "name": "codename/core-ui",
    "description": "This is going to be the core UI framework.",
    "type": "library",
    "keywords": ["codename", "core", "ui"],
    "authors": [
        {
            "name": "Kevin Dargel",
            "role": "Software Developer"
        }
    ],
    "require": {
      "codename/core" : "*",
      "twig/twig" : "2.13.1",
      "twig/extensions" : "^1.5",
      "dompdf/dompdf" : "^1.0"
    },
    "require-dev" : {
      "phpunit/phpunit" : "^9.0",
      "codename/core-test" : "*"
    },
    "autoload": {
      "psr-4": {
          "codename\\core\\ui\\": "backend/class/"
      }
    },
    "autoload-dev": {
      "psr-4": {
          "codename\\core\\io\\tests\\": "tests/"
      }
    }
}
